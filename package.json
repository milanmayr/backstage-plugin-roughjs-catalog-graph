{
  "name": "@milanmayr/backstage-plugin-catalog-graph-excalidraw",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "directory": "."
  },
  "homepage": "https://github.com/milanmayr/backstage-plugin-catalog-graph-excalidraw",
  "keywords": [
    "backstage",
    "plugin",
    "catalog",
    "graph",
    "excalidraw"
  ],
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.esm.js",
        "require": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "catalog-graph-excalidraw",
    "pluginPackages": [
      "@milanmayr/backstage-plugin-catalog-graph-excalidraw"
    ]
  },
  "sideEffects": false,
  "scripts": {
    "start": "backstage-cli package start",
    "build": "backstage-cli package build && yarn build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap --skipLibCheck 2>&1 | grep -v 'node_modules/react-markdown' | grep -v 'Cannot find namespace' | grep -v 'Type.*TagName.*does not satisfy' | grep -v 'Type.*string.*is not assignable.*ElementType' | grep -v 'Type.*TagName.*is not assignable.*FunctionComponent' | grep -v 'Type.*string.*is not assignable.*FunctionComponent' || true",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack && yarn build:types",
    "postpack": "backstage-cli package postpack"
  },
  "dependencies": {
    "@backstage/catalog-model": "^1.7.0",
    "@backstage/core-components": "^0.18.2",
    "@backstage/plugin-catalog-react": "^1.15.0",
    "@backstage/theme": "^0.7.0",
    "@material-ui/core": "^4.12.2",
    "d3-shape": "^3.2.0",
    "react-router-dom": "^7.13.0",
    "roughjs": "^4.6.6"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.35.3",
    "@backstage/core-app-api": "^1.19.1",
    "@backstage/dev-utils": "^1.1.16",
    "@backstage/plugin-catalog-graph": "^0.5.2",
    "@backstage/test-utils": "^1.7.12",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/d3-shape": "^3.1.8",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "react": "^18.0.2",
    "react-dom": "^18.0.2"
  },
  "files": [
    "dist"
  ]
}
